{
    "apps": [
        {
            "name": "myapp",
            "cwd": "./",
            "script": "./bin/www",
            "log_date_format": "YYYY-MM-DD HH:mm Z",
            "error_file": "./log/err.log",
            "log_file": "./log/log.log",
            "out_file": "./log/out.log",
            "pid_file": "./log/pid.log",
            "min_uptime": "60s",
            "max_restarts": 100,
            "max_memory_restart": "300M",
            "cron_restart": "1 0 * * *",
            "watch": false,
            "merge_logs": true,
            "exec_interpreter": "node",
            "exec_mode": "fork",
            "autorestart": true,
            "vizion": false
    }
  ]
}
